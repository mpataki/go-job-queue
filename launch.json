{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Server",
            "type": "delve",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/service/cmd/server",
            "env": {
                "REDIS_ADDR": "localhost:6379"
            },
            "args": []
        },
        {
            "name": "Debug Current Test",
            "type": "delve",
            "request": "launch",
            "mode": "test",
            "program": "${file}"
        },
        {
            "name": "Debug Package Tests",
            "type": "delve",
            "request": "launch",
            "mode": "test",
            "program": "${fileDirname}"
        },
        {
            "name": "Attach to Running Process",
            "type": "delve",
            "request": "attach",
            "mode": "local",
            "processId": 0
        }
    ]
}
